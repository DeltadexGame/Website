               \n\n\t\t\t\t\t  this.dex[key].quant -= 1\n\t\t\t\t\t  this.count -= 1;\n\t\t\t\t\t  if (obj.quant === 0){\n\t\t\t\t\t\tthis.dex.splice(key, 1);\n\t\t\t\t\t  }\n\t\t\t\t\t\n\t\t\t\t  }\n\t\t\t\t\t\n\t\t\t\t  }\n\t\t\t\t \n\n\t},\n\t\t  handleClickInParent: function(name, img) {\n\t\t\tthis.present = false;\n\t\t\tif (this.count < 10) {   //Search          \n\t\t\t  for (var key in this.dex) {    \n\t\t\t\t  var obj = this.dex[key];\n\t\t\t\t\t\t\tif (name == obj.name){                \n\n\t\t\t\t\t\t\t\tthis.dex[key].quant += 1\n\t\t\t\t\t\t\t\tthis.present = true;\n\t\t\t\t\t\t\t\tthis.count += 1;\n\t\t\t\t\t\t\t\t\n\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\n\t\t\t\t  }\n\t\t\t\t  if (this.present == false){\n\t\t\t\t\tthis.dex.push({ name: name, img: img,   quant: 1 });\n\t\t\t\t\tthis.count += 1;\n\t\t\t\t  } ;\n\t\t\t\t   this.present = 0;\n\t\t\t\t\t\n\t\t\t\t  }; \n\n\t}\n\t\t\n\t\t},\n\t  components: {\n\t\tproduct\n\t  }\n\t}\n</script>\n<style scoped>\nimg{\n\t  height: 1em;\n\t  width: 1em;\n\t}\n\n\taside{\n\t  border-style: inset;\n\t  background-color: white;\n\t  left: 90%;\n\t  position: fixed;\n\t  top: 0;\n\t  padding-bottom:100%;\n\t}\n\n\t.product{\n\t  height:250px;\n\t  width:200px;\n\t}\n</style>"],"sourceRoot":"src/components"}]}